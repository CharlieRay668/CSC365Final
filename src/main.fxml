<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="712.0" prefWidth="1054.0" style="-fx-background-color: #B0C4DE;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <TextField id="trackString" fx:id="trackString" layoutX="189.0" layoutY="165.0" prefHeight="47.0" prefWidth="338.0" promptText="Enter Track Name">
         <font>
            <Font name="Dubai Regular" size="19.0" />
         </font>
      </TextField>
      <ListView id="trackList" fx:id="mainSearchResults" layoutX="189.0" layoutY="212.0" onMouseClicked="#chooseTrack" prefHeight="351.0" prefWidth="401.0" />
      <Button id="searchButton" layoutX="527.0" layoutY="165.0" mnemonicParsing="false" onAction="#searchMain" prefHeight="47.0" prefWidth="64.0" text="Search">
         <font>
            <Font name="Dubai Regular" size="15.0" />
         </font>
      </Button>
      <Button layoutX="972.0" layoutY="8.0" mnemonicParsing="false" onAction="#switchToPlaylists" prefHeight="37.0" prefWidth="64.0" text="Playlists" />
      <Button layoutX="908.0" layoutY="8.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="64.0" text="Home" />
      <MenuButton fx:id="addToPlaylistButton" layoutX="591.0" layoutY="165.0" mnemonicParsing="false" onAction="#initialize" prefHeight="47.0" prefWidth="167.0" text="Add To Playlist">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
         <font>
            <Font name="Dubai Regular" size="18.0" />
         </font>
      </MenuButton>
   </children>
</AnchorPane>
